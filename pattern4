//A A A A A 
B B B B 
C C C 
D D 
E 


public class Pattern4 {
    public static void main(String[] args) {
int rows = 5;
char ch = 'A';
for (int i = rows; i >= 1; i--) {
for (int j = 1; j <= i; j++) {
System.out.print(ch + " ");
            }
System.out.println();
            ch++;
        }
    }
}
